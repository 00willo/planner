#: libplanner/mrp-project.c:441
msgid "Default"
msgstr "Predeterminado"

#: libplanner/mrp-project.c:749
#, c-format
msgid "Couldn't find a suitable file module for loading '%s'"
msgstr "No se pudo encontrar un módulo para cargar «%s»"

#: libplanner/mrp-project.c:770 libplanner/mrp-project.c:1034
msgid "No support for SQL storage built into this version of MrProject."
msgstr ""
"No hay soporte para almacenes SQL compilado en esta versión de Mr Project."

#: libplanner/mrp-project.c:807
msgid "Invalid URI."
msgstr "URI inválida"

#: libplanner/mrp-project.c:928
#, c-format
msgid "Unable to find file writer identified by '%s'"
msgstr "No es posible encontrar el escritor de archivos identificado por «%s»"

#: libplanner/mrp-project.c:991
msgid "Couldn't find a suitable file module for loading project"
msgstr ""
"No se pudo encontrar un módulo de archivo adecuado para cargar el proyecto"

#: libplanner/mrp-property.c:365
msgid "Text"
msgstr "Texto"

#: libplanner/mrp-property.c:367
msgid "String list"
msgstr "Lista de cadenas"

#: libplanner/mrp-property.c:369
msgid "Integer number"
msgstr "Número entero"

#: libplanner/mrp-property.c:371
msgid "Floating-point number"
msgstr "Número de punto flotante"

#: libplanner/mrp-property.c:373
msgid "Date"
msgstr "Fecha"

#: libplanner/mrp-property.c:375
msgid "Duration"
msgstr "Duración"

#: libplanner/mrp-property.c:377
msgid "Cost"
msgstr "Costo"

#: libplanner/mrp-property.c:380
msgid "None"
msgstr "Ninguno"

#: libplanner/mrp-task-manager.c:1769
msgid "Can not add a predecessor relation between a task and its ancestor."
msgstr ""
"No se puede añadir una relación predecesora entre una tarea y sus ancestros."

#: libplanner/mrp-task-manager.c:1778 libplanner/mrp-task-manager.c:1786
msgid "Can not add a predecessor, because it would result in a loop."
msgstr "No se puede añadir un predecesor debido a que formaría un ciclo."

#: libplanner/mrp-task-manager.c:1821
msgid "Can not move the task, since it would result in a loop."
msgstr "No se puede mover la tarea, debido a que formaría un ciclo."

#: libplanner/mrp-task.c:1016
msgid ""
"Could not add a predecessor relation, because the tasks are already related."
msgstr ""
"No se puede añadir una relación predecesora, debido a que las tareas ya se "
"encuentran relacionadas."

#: libplanner/mrp-day.c:54
msgid "Working"
msgstr "Trabajo"

#: libplanner/mrp-day.c:55
msgid "A default working day"
msgstr "Un día de trabajo predeterminado"

#: libplanner/mrp-day.c:57
msgid "Nonworking"
msgstr "Descanso"

#: libplanner/mrp-day.c:58
msgid "A default non working day"
msgstr "Un día predeterminado de descanso"

#: libplanner/mrp-day.c:60
msgid "Use base"
msgstr "Usar base"

#: libplanner/mrp-day.c:61
msgid "Use day from base calendar"
msgstr "Usar día desde el calendario base"

#: libplanner/mrp-parser.c:1043 libplanner/mrp-parser.c:1138
msgid "Could not create XML tree"
msgstr "No se puede crear el árbol XML"

#: libplanner/mrp-parser.c:1104
msgid "Could not write XML file"
msgstr "No se puede escribir el archivo XML"

#. Give up.
#: libplanner/mrp-sql.c:229
msgid "Invalid Unicode"
msgstr "Unicode inválido"

#: libplanner/mrp-sql.c:2205
#, c-format
msgid ""
"The project '%s' has been changed by the user '%s' since you opened it. Do "
"you want to save anyway?"
msgstr ""
"El proyecto «%s» ha sido modificado por el usuario «%s» desde la última vez "
"que lo abrió. ¿Quiere guardarlo de todas formas?"

#: libplanner/mrp-sql.c:3443
#, c-format
msgid ""
"Connection to database '%s' failed.\n"
"%s"
msgstr ""
"La conexión a la base de datos «%s» ha fallado. \n"
"%s"

#: libplanner/mrp-storage-sql.c:205
msgid "Invalid SQL URI (must start with 'sql://' and contain '#')."
msgstr "URI SQL inválida (debe empezar con 'sql://' y contener '#'),"

#: libplanner/mrp-storage-sql.c:274
msgid "Invalid SQL URI (invalid project id)."
msgstr "URI SQL Inválida (id de proyecto inválido)"

#: libplanner/mrp-storage-sql.c:282
msgid "Invalid SQL URI (no database name specified)."
msgstr "URI SQL inválida (no hay especificado un nombre de base de datos)."

